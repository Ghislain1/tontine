{
  "name": "tontine",
  "version": "14.1.2",
  "scripts": {
    "ng": "ng",
    "start": "cls && ng serve --port 4200 --open --verbose",
    "build": "ng build --verbose true",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "deploy": "ng build --output-path docs --base-href /tontine/",
    "create-github-pages": "npm run deploy && npm run copy-index",
    "coverage": "ng test --no-watch --code-coverage",
    "serve": "node server",
    "dev": "npm run build:prod &&  git add * && git commit -m Updating...  && git push",
    "lint": "ng lint",
    "build:prod": "ng build --configuration production --base-href https://ghislain1.github.io/tontine/ --verbose true",
    "build:prod2": "ng build --configuration production --output-path docs --base-href ./ --verbose true",
    "test:headless": "ng test --watch=false --browsers=ChromeHeadless"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.1.2",
    "@angular/common": "14.1.2",
    "@angular/compiler": "14.1.2",
    "@angular/core": "14.1.2",
    "@angular/forms": "14.1.2",
    "@angular/platform-browser": "14.1.2",
    "@angular/platform-browser-dynamic": "14.1.2",
    "@angular/router": "14.1.2",
    "@fortawesome/fontawesome-free": "^6.2.0",
    "mdb-angular-ui-kit": "^2.3.0",
    "rxjs": "7.5.6",
    "tslib": "2.4.0",
    "zone.js": "0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.1.2",
    "@angular-eslint/builder": "14.1.2",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "14.1.2",
    "@angular-eslint/schematics": "14.1.2",
    "@angular-eslint/template-parser": "14.1.2",
    "@angular/cli": "14.1.2",
    "@angular/compiler-cli": "14.1.2",
    "@types/jasmine": "^4.3.0",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "eslint": "8.23.1",
    "jasmine-core": "4.4.0",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "tslib": "^2.4.0",
    "typescript": "4.6.2"
  }
}
